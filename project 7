#include <iostream>

using namespace std;

int main()
{
    int a;
    int b;
    cout << "input the first number = " << a << endl;
    cin >> a;
    cout << "input the second number = " << b << endl;
    cin >> b;
    cout << "after swapping the first number = " << b << endl;
    cout << "after swapping the second number = " << a << endl;
    return 0;
}
