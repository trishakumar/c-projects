#include <iostream>
#include<cmath>
using namespace std;

int main()
{
    int a, b, c;
    float s;
    cout << "enter the value of s " << endl;
    cin >> s;
    cout << "enter the sides = " << endl;
    cin >> a, b, c;
    cout << "area of triangle = " << sqrt(s*(s-a)*(s-b)*(s-c)) << endl;
    return 0;
}
