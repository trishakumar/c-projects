#include <iostream>
#include<iomanip>
using namespace std;

int main()
{
    int i1 = 4, i2 = 8;


    cout << i1/i2 << endl;
    cout << (double)i1/i2 << endl;
    cout << (double) (i1/12) << endl;

    double d1= 5.5, d2 = 6.6;
    cout << (int)d1/d2 << endl;
    cout << (int)(d1/d2) << endl;
    return 0;
}
